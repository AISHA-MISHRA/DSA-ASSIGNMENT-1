#include <iostream>
#include <string>
using namespace std;

int main() {
	// Complete the program
    string a;
    string b;
    cin>>a>>b;
    int len =a.size();
    int len1=b.size();
    cout<<len<<" "<<len1<<endl; ;
    string c=a+b;
    cout<<c<<"\n";
    char x=a[0];
    a[0]=b[0];
    b[0]=x;
    cout<<a<<" "<<b<<endl;
    return 0;
}
